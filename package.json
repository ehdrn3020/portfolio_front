  "license": "MIT",
  "dependencies": {
    "gh-pages": "^4.0.0",
    "gsap": "^3.5.1",
    "imagesloaded": "^4.1.4",
    "locomotive-scroll": "^4.0.6",
    "parcel-bundler": "^1.12.4",
    "src": "^1.1.2"
  },
  "homepage": "https://ehdrn3020.github.io/portfolio_front/"
}
