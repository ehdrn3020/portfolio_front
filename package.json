{
  "name": "DGK Portfolio FrontEnd",
  "version": "1.0.0",
  "description": "*My Work Projects Gallery*",
  "main": "src/js/index.js",
  "scripts": {
    "start": "parcel index.html --open",
    "clean": "rm -rf dist/*",
    "build:parcel": "parcel build index.html --no-content-hash --no-minify --no-source-maps --public-url ./",
    "build": "npm run clean && npm run build:parcel",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ehdrn3020/portfolio_front.git"
  },
  "keywords": [],
